#################################################################
#    Object Properties
#################################################################


###  https://w3id.org/nsd#hasVisualTemplate
nsd:hasVisualTemplate rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf schema:image ;
                      rdfs:domain nsd:StoneDamage ;
                      rdfs:range nsd:VisualTemplate .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/nsd#appearance
nsd:appearance rdf:type owl:DatatypeProperty ;
               rdfs:domain nsd:GapDamage ;
               rdfs:comment "beschreibt das Aussehen des detektierten Schadens"@de ,
                            "describes the appearance of the detected damage."@en ;
               rdfs:label "appearance"@en ,
                          "aussehen"@de .


###  https://w3id.org/nsd#color
nsd:color rdf:type owl:DatatypeProperty ;
          rdfs:domain nsd:DiscolorationDeposit ;
          rdfs:comment "Beschreibt die Farbe einer detektierten Ablagerung"@de ,
                       "Describes the color of a detected discoloration or deposit"@en ;
          rdfs:label "Farbe"@de ,
                     "color"@en .


###  https://w3id.org/nsd#colorChange
nsd:colorChange rdf:type owl:DatatypeProperty ;
                rdfs:domain nsd:DiscolorationDeposit ;
                rdfs:comment "Aussage ob eine Farbveränderung in der Ablagerung erkennbar ist"@de ,
                             "States whether a color change is detected in the deposit or not."@en ;
                rdfs:label "Farbveränderung"@de ,
                           "color change"@en .


###  https://w3id.org/nsd#crackDepth
nsd:crackDepth rdf:type owl:DatatypeProperty ;
               rdfs:comment "defines the depth of a crack in [mm]"@en ,
                            "definiert die Tiefe eines Risses in [mm]"@de ;
               rdfs:label "Risstiefe"@de ,
                          "crack depth"@en ;
               schema:domainIncludes nsd:GapDamage ,
                                     nsd:StoneDamage .


###  https://w3id.org/nsd#crackDirection
nsd:crackDirection rdf:type owl:DatatypeProperty ;
                   rdfs:domain nsd:Crack ;
                   rdfs:comment "beschreibt die Ausrichtung eines Risses"@de ,
                                "defines the direction or alignment of a crack"@en ;
                   rdfs:label "Rissausrichtung"@de ,
                              "crack direction"@en .


###  https://w3id.org/nsd#crackLength
nsd:crackLength rdf:type owl:DatatypeProperty ;
                rdfs:domain nsd:Crack ;
                rdfs:comment "defines the length of a crack in [mm]"@en ,
                             "definiert die Länge eines Risses in [mm]"@de ;
                rdfs:label "Risslänge"@de ,
                           "crack length"@en .


###  https://w3id.org/nsd#crackNumber
nsd:crackNumber rdf:type owl:DatatypeProperty ;
                rdfs:comment "Beschreibt, die Menge erfasster Risse für einen Rissbereich"@de ,
                             "Describes the amount of cracks for a damage area"@en ;
                rdfs:label "Rissanzahl"@de ,
                           "crack amount"@en ;
                schema:domainIncludes nsd:GapDamage ,
                                      nsd:StoneDamage .


###  https://w3id.org/nsd#crackSpacing
nsd:crackSpacing rdf:type owl:DatatypeProperty ;
                 rdfs:domain nsd:Crack ;
                 rdfs:comment "defines the crack spacing of multiple cracks in a damage area in [mm]"@en ,
                              "definiert den Abstand von mehreren Rissen in einem Schadensbereich in [mm]"@de ;
                 rdfs:label "Rissabstand"@de ,
                            "crack spacing"@en .


###  https://w3id.org/nsd#crackWidth
nsd:crackWidth rdf:type owl:DatatypeProperty ;
               rdfs:domain nsd:Crack ;
               rdfs:comment "defines the width of a crack in [mm]"@en ,
                            "definiert die Breite eines Risses in [mm]"@de ;
               rdfs:label "Rissbreite"@de ,
                          "crack width"@en .


###  https://w3id.org/nsd#damageArea
nsd:damageArea rdf:type owl:DatatypeProperty ;
               rdfs:comment "defines the area of a damage area in [cm²]"@en ,
                            "definiert die Fläche eines Schadensareals in [cm²]"@de ;
               rdfs:label "Schadensfläche"@de ,
                          "damage area"@en ;
               schema:domainIncludes nsd:MechanicalDamage ,
                                     nsd:StructuralLoss .


###  https://w3id.org/nsd#damageExtend
nsd:damageExtend rdf:type owl:DatatypeProperty ;
                 schema:domainIncludes nsd:GapDamage ,
                                       nsd:StoneDamage .


###  https://w3id.org/nsd#damageImpact
nsd:damageImpact rdf:type owl:DatatypeProperty ;
                 rdfs:comment "Index zur Beschreibung des Schadensausmaßes"@de ,
                              "index for describing the impact of a damage"@en ;
                 rdfs:label "Schadensausmaß"@de ,
                            "damage impact"@en ;
                 schema:domainIncludes nsd:GapDamage ,
                                       nsd:StoneDamage .


###  https://w3id.org/nsd#damageIntensity
nsd:damageIntensity rdf:type owl:DatatypeProperty ;
                    rdfs:comment "Index zur Beschreibung der Schadensintensität"@de ,
                                 "index for describing the intensity of a damage"@en ;
                    rdfs:label "Schadensintensität"@de ,
                               "damage intensity"@en ;
                    schema:domainIncludes nsd:GapDamage ,
                                          nsd:StoneDamage .


###  https://w3id.org/nsd#damageLength
nsd:damageLength rdf:type owl:DatatypeProperty ;
                 rdfs:domain nsd:GapDamage ;
                 rdfs:comment "defines the length of a damage in [mm]"@en ,
                              "definiert die Länge eines Schadens in [mm]"@de ;
                 rdfs:label "Schadenslänge"@de ,
                            "damage length"@en .


###  https://w3id.org/nsd#damageLocation
nsd:damageLocation rdf:type owl:DatatypeProperty ;
                   rdfs:domain nsd:StructuralLoss ;
                   rdfs:comment "Defines whether a damage is located on the surface or edge of a stone."@en ,
                                "Definiert ob ein Schaden an der Steinfläche oder Steinkanten lokalisiert ist."@de ;
                   rdfs:label "Schadensverortung"@de ,
                              "damage location"@en .


###  https://w3id.org/nsd#deposit
nsd:deposit rdf:type owl:DatatypeProperty ;
            rdfs:domain nsd:DiscolorationDeposit ;
            rdfs:comment "Defines whether a deposit is located or not"@en ,
                         "Definiert, ob eine Ablagerung im Stein verortet wurde oder nicht."@de ;
            rdfs:label "Ablagerung vorhanden"@de ,
                       "deposit existent"@en .


###  https://w3id.org/nsd#descriptionContent
nsd:descriptionContent rdf:type owl:DatatypeProperty .


###  https://w3id.org/nsd#layerColor
nsd:layerColor rdf:type owl:DatatypeProperty ;
               rdfs:domain nsd:BiologicalColonisation ;
               rdfs:comment "Beschreibt die Farbe einer Schicht von einer Biologischen Besiedlung"@de ,
                            "Describes the color of a layer of a biological colonisation"@en ;
               rdfs:label "Schichtfarbe"@de ,
                          "layer color"@en .


###  https://w3id.org/nsd#layerThickness
nsd:layerThickness rdf:type owl:DatatypeProperty ;
                   rdfs:domain nsd:BiologicalColonisation ;
                   rdfs:comment "Defines the layer thickness of a biological colonisation in [mm]"@en ,
                                "Definiert die Dicke einer Schicht einer Biologischen Besiedlung in [mm]"@de ;
                   rdfs:label "Schichtdicke"@de ,
                              "layer thickness"@en .


###  https://w3id.org/nsd#materialLoss
nsd:materialLoss rdf:type owl:DatatypeProperty ;
                 rdfs:comment "Beschreibt das Ausmaß des Materialverlustes"@de ,
                              "Describes the extent of a detected material loss."@en ;
                 rdfs:label "Materialverlust"@de ,
                            "material loss"@en ;
                 schema:domainIncludes nsd:GapDamage ,
                                       nsd:StoneDamage .


###  https://w3id.org/nsd#numDiffDamages
nsd:numDiffDamages rdf:type owl:DatatypeProperty ;
                   rdfs:domain nsd:StoneDamage ;
                   rdfs:comment "attribute to store the number of different damages for each stone"@en ,
                                "gibt die Anzahl unterschiedlicher Schadenstypen für einen Steinschaden an"@de ;
                   rdfs:label "Amount of different damages"@en ,
                              "Anzahl unterschiedlicher Schäden"@de .


###  https://w3id.org/nsd#relToStoneStructure
nsd:relToStoneStructure rdf:type owl:DatatypeProperty ;
                        rdfs:domain nsd:Detachment .


###  https://w3id.org/nsd#structuralEffect
nsd:structuralEffect rdf:type owl:DatatypeProperty ;
                     rdfs:domain nsd:Crack .


#################################################################
#    Classes
#################################################################


###  https://w3id.org/nsd#Abrasion
nsd:Abrasion rdf:type owl:Class ;
             rdfs:subClassOf nsd:MechanicalDamage ;
             rdfs:comment "Eine Form der Erosion, die durch Abtragen oder Abreiben mit harten Werkzeugen oder durch den Aufprall von Partikeln (Sandstrahl) erzeugt wird."@de ,
                          "Erosion due to wearing down or rubbing away by means of friction, or to the impact of particles."@en ;
             rdfs:label "Abrasion"@en ,
                        "Abrieb"@de .


###  https://w3id.org/nsd#Alga
nsd:Alga rdf:type owl:Class ;
         rdfs:subClassOf nsd:BiologicalColonisation ;
         rdfs:comment """Algae are microscopic vegetal organisms without stem or leaves which can be seen outdoors and indoors, as powdery or viscous deposits (thickness :
tenth of mm to several mm). Algae form green, red, brown, or black veil like zones and can be found mainly in situations where the substrate remains
moistened for long periods of time. Depending on the environmental conditions and substrate type, algae may form solid layers or smooth films. On monuments, algae are constituted of unicellular to pluricellular clusters, and they never form macroorganisms."""@en ,
                      "Pflanzlicher Mikroorganismus ohne Stengel und Blätter, der sowohl im Freien als auch in In nen räumen in Gestalt von pulvrigen bis viskosen Ablagerungen von einem Zehntel bis einige Millimeter Dicke gefunden wird. Algen bilden grüne, rote, braune oder schwarze, schleierartige Überzüge und kommen hauptsächlich dort vor, wo das Substrat längere Zeit feucht bleibt. Abhängig von den Umgebungsbedingungen und vom Substrat bilden Algen kompakte oder feine, filmartige Überzüge. Die an Denkmälern auftretenden Algen bestehen aus einzelligen bis vielzelligen Clustern; sie bilden keine Makroorganismen aus."@de ;
         rdfs:label "Alga"@en ,
                    "Algen"@de .


###  https://w3id.org/nsd#Alveolization
nsd:Alveolization rdf:type owl:Class ;
                  rdfs:subClassOf nsd:StructuralLoss ;
                  rdfs:comment "Describes a deterioration pattern even if there is no active material loss at the time the object is described."@en ,
                               "Schadensformen, welche auf Schadensprozesse zurückgehen, die zur Beobachtungszeit nicht mehr aktiv sind."@de ;
                  rdfs:label "Alveolenbildung"@de ,
                             "Features induced by material loss"@en .


###  https://w3id.org/nsd#BiologicalColonisation
nsd:BiologicalColonisation rdf:type owl:Class ;
                           rdfs:subClassOf nsd:StoneDamage ;
                           rdfs:comment """Colonization of the stone by plants and micro-organisms such as bacteria, cyanobacteria, algae, fungi and lichen (symbioses of the latter three).
Biological colonization also includes influences by other organisms such as animals nesting on and in stone."""@en ,
                                        """Die Besiedlung von Gesteinen durch höhere Pflanzen und Mikroorganismen wie Bakterien, Cyanobakterien, Algen, Pilze und Flechten (Symbiosen zwischen
den drei Letztgenannten). Biologische Besiedlung umfasst auch Einflüsse durch andere Organismen wie den Nestbau von Tieren auf und im Stein."""@de ;
                           rdfs:label "Biological Colonisation"@en ,
                                      "Biologische Besiedlung"@de .


###  https://w3id.org/nsd#BlackCrust
nsd:BlackCrust rdf:type owl:Class ;
               rdfs:subClassOf nsd:Crust ;
               rdfs:comment """Kind of crust developing generally on areas protected against direct rainfall or water runoff in urban environment. Black crusts usually adhere firmly to the substrate. They are composed mainly of particles from the atmosphere,
trapped into a gypsum matrix."""@en ,
                            "entwickeln sich allgemein im urbanen Umfeld auf Flächen, die vor direktem Regen und Ablaufwasser geschützt sind. Sie haften gewöhnlich sehr fest auf dem Substrat. Sie setzen sich hauptsächlich aus Partikeln aus der Luft zusammen, die in einer Gipsmatrix fixiert sind."@de ;
               rdfs:label "Black crust"@en ,
                          "Schwarze Kruste"@de .


###  https://w3id.org/nsd#Blistering
nsd:Blistering rdf:type owl:Class ;
               rdfs:subClassOf nsd:Detachment ;
               rdfs:comment "Abgetrennte, luftgefüllte, erhabene, halbkugelförmige Erhebung auf der Steinoberfläche, entstanden durch die Ablösung einer äußeren Gesteinsschicht. Die Ablösung ist unabhängig von der Gesteinstextur."@de ,
                            "Separated, air-filled, raised hemispherical elevations on the face of stone resulting from the detachment of an outer stone layer. This detachment is not related to the stone structure."@en ;
               rdfs:label "Blasenbildung / Aufwölbung"@de ,
                          "Blistering"@en .


###  https://w3id.org/nsd#Bursting
nsd:Bursting rdf:type owl:Class ;
             rdfs:subClassOf nsd:Detachment ;
             rdfs:comment "Local loss of the stone surface from internal pressure usually manifesting in the form of an irregularly-sided crater."@en ,
                          "Lokaler Verlust der Steinoberfläche durch inneren Druck, im Allgemeinen in Form eines ungleichmäßig begrenzten Kraters."@de ;
             rdfs:label "Ausbruch"@de ,
                        "Bursting"@en .


###  https://w3id.org/nsd#Chipping
nsd:Chipping rdf:type owl:Class ;
             rdfs:subClassOf nsd:Fragmentation ;
             rdfs:comment "Ausbrechen von Stücken (Chips, Splitter) von den Ecken eines Gesteinsblocks."@de ,
                          "Breaking off of pieces, called chips, from the edges of a block."@en ;
             rdfs:label "Absplittern"@de ,
                        "Chipping"@en .


###  https://w3id.org/nsd#Colouration
nsd:Colouration rdf:type owl:Class ;
                rdfs:subClassOf nsd:DiscolorationDeposit ;
                rdfs:comment "change in hue, value and/or a gain in chroma"@en ,
                             "eine Veränderung in Farbwert, Helligkeit und/oder eine Zunahme der Farbsättigung."@de ;
                rdfs:label "Colouration"@en ,
                           "Färbung"@de .


###  https://w3id.org/nsd#ConcaveDeformation
nsd:ConcaveDeformation rdf:type owl:Class ;
                       rdfs:subClassOf nsd:Deformation ;
                       rdfs:comment "Deformation with a concave bulge."@en ,
                                    "Verformung mit einer konkaven Wölbung."@en ;
                       rdfs:label "Concave Deformation"@en ,
                                  "Konkave Verformung"@de .


###  https://w3id.org/nsd#ContourScaling
nsd:ContourScaling rdf:type owl:Class ;
                   rdfs:subClassOf nsd:Scaling ;
                   rdfs:comment "Hier verläuft die Trennfläche zwischen Schale und gesundem Gestein parallel zur Gesteinsoberfläche. Im Fall von ebenen Oberflächen kann die Konturschale auch als „Abplatzung“ bezeichnet werden. „Case Hardening“ („Buchdeckelverhärtung“) ist ein Synonym für Konturschale."@de ,
                                "scaling in which the interface with the sound part of the stone is parallel to the stone surface. In the case of flat surfaces, contour scaling may be called spalling. Case hardening is a synonym of contour scaling."@en ;
                   rdfs:label "Contour scaling"@en ,
                              "Konturschale"@de .


###  https://w3id.org/nsd#ConvexDeformation
nsd:ConvexDeformation rdf:type owl:Class ;
                      rdfs:subClassOf nsd:Deformation ;
                      rdfs:comment "Deformation with a convex bulge."@en ,
                                   "Verformung mit einer konvexen Wölbung."@en ;
                      rdfs:label "Convex Deformation"@en ,
                                 "Konvexe Verformung"@de .


###  https://w3id.org/nsd#Coving
nsd:Coving rdf:type owl:Class ;
           rdfs:subClassOf nsd:StructuralLoss ;
           rdfs:comment "Describes a deterioration pattern even if there is no active material loss at the time the object is described."@en ,
                        "Schadensformen, welche auf Schadensprozesse zurückgehen, die zur Beobachtungszeit nicht mehr aktiv sind."@de ;
           rdfs:label "Alveolenbildung"@de ,
                      "Features induced by material loss"@en .


###  https://w3id.org/nsd#Crack
nsd:Crack rdf:type owl:Class ;
          rdfs:subClassOf nsd:StoneDamage ;
          rdfs:comment "Einzelner Spalt (Trennung), der deutlich mit dem Auge sichtbar ist. Entsteht durch die Trennung von zwei Teilen."@de ,
                       "Individual fissure, clearly visible by the naked eye, resulting from separation of one part from another."@en ;
          rdfs:label "Crack"@en ,
                     "Riss"@de .


###  https://w3id.org/nsd#Craquele
nsd:Craquele rdf:type owl:Class ;
             rdfs:subClassOf nsd:Crack ;
             rdfs:comment "Network of minor cracks also called crack network. The term crazing is not appropriate for stone, as this term should be used for describing the development of a crack network on glazed terracotta."@en ,
                          "auch Rissnetz genannt, ist ein Netzwerk von kleineren Rissen. Der Begriff „Glasurrisse“ ist für Gesteine nicht geeignet, weil dieser Begriff nur benützt werden sollte, um die Bildung von Netzwerkrissen auf glasierter Terrakotta zu beschreiben."@de ;
             rdfs:label "Craquele"@de ,
                        "Craquele"@en .


###  https://w3id.org/nsd#Crumbling
nsd:Crumbling rdf:type owl:Class ;
              rdfs:subClassOf nsd:Disintegration ;
              rdfs:comment "Ablösung von Kornaggregaten vom Substrat. Diese Aggregate besitzen allgemein eine begrenzte Größe von unter 2 cm. Die Größe hängt vom Gestein und von den Umgebungsbedingungen ab."@de ,
                           "Detachment of aggregates of grains from the substrate. These aggregates are generally limited in size (less than 2 cm). This size depends on the nature of the stone and its environment."@en ;
              rdfs:label "Abbröckeln"@de ,
                         "Crumbling"@en .


###  https://w3id.org/nsd#Crust
nsd:Crust rdf:type owl:Class ;
          rdfs:subClassOf nsd:DiscolorationDeposit ;
          rdfs:comment "Allgemein formuliert ist eine Kruste eine zusammenhängende Ansammlung von Stoffen auf der Oberfläche. Eine Kruste kann exogene Ablagerungen in Verbindung mit Stoffen aus dem Stein verbinden. Krusten sind häufig schwarz gefärbt („Schwarze Kruste“), aber es finden sich auch helle Krusten. Krusten können eine gleichmäßige Dicke aufweisen und die Steinoberfläche nachzeichnen; sie können aber auch eine unregelmäßige Dicke haben, weshalb Details der Gesteinsoberfläche dann schwerer zu erkennen sind."@de ,
                       """Generally coherent accumulation of materials on the surface. A crust may include exogenic deposits in combination with materials derived from the stone.
A crust is frequently dark coloured (black crust) but light colours can also be found. Crusts may have an homogeneous thickness, and thus replicate the
stone surface, or have irregular thickness and disturb the reading of the stone surface details."""@en ;
          rdfs:label "Crust"@en ,
                     "Kruste"@de .


###  https://w3id.org/nsd#Cut
nsd:Cut rdf:type owl:Class ;
        rdfs:subClassOf nsd:MechanicalDamage ;
        rdfs:comment "Loss of material due to the action of an edge tool. It can have the appearance of an excavated cavity, an incision, a missing edge, etc...Tool marks can be considered as special kinds of cuts but should not be considered as damage features."@en ,
                     "Verlust von Steinmaterial durch ein scharfkantiges Werkzeug. Ein Einschnitt kann die Form einer ausgearbeiteten Aushöhlung, einer Einritzung, einer fehlenden Kante etc. haben. Werkzeugspuren können als eine besondere Form von Einschnitten angesehen werden, sollten aber nicht zu den Schäden gezählt werden."@de ;
        rdfs:label "Cut"@en ,
                   "Einschnitt"@de .


###  https://w3id.org/nsd#Deformation
nsd:Deformation rdf:type owl:Class ;
                rdfs:subClassOf nsd:StoneDamage ;
                rdfs:comment "Change in shape without losing integrity, leading to bending, buckling or twisting of a stone block."@en ,
                             "Formänderung bei intakt bleibendem Gefüge, die zur Verbiegung, Aufwölbung oder Verdrehung eines Steinteils führt."@de ;
                rdfs:label "Deformation"@en ,
                           "Verformung"@de .


###  https://w3id.org/nsd#Delamination
nsd:Delamination rdf:type owl:Class ;
                 rdfs:subClassOf nsd:Detachment ;
                 rdfs:comment "Detachment process affecting laminated stones (most of sedimentary rocks, some metamorphic rocks). It corresponds to a physical separation into one or several layers following the stone laminae. The thickness and the shape of the layers are variable. The layers may be oriented in any direction with regards to the stone surface."@en ,
                              """Prozess der Gesteinsablösung, der geschichtete Gesteine angreift (zumeist Sedimentgesteine, einige metamorphe Gesteine). Schichtspaltung entspricht einer physikalischen Trennung in einer oder mehrere Lagen, welche der Gesteinsschichtung folgen. Die Dicke und Form der Schichten sind variabel. Die Schichten können in jeglicher Richtung bezüglich der Steinoberfläche
orientiert sein."""@de ;
                 rdfs:label "Delamination"@en ,
                            "Schichtspaltung"@de .


###  https://w3id.org/nsd#Deposit
nsd:Deposit rdf:type owl:Class ;
            rdfs:subClassOf nsd:DiscolorationDeposit ;
            rdfs:comment """Accumulation of exogenic material of variable thickness. Some examples of deposits : splashes of paint or mortar, sea salt aerosols, atmospheric particles
such as soot or dust, remains of conservation materials such as cellulose poultices, blast materials, etc."""@en ,
                         "Ansammlung von exogenem Material variabler Dicke. Einige Beispiele von Ablagerungen: Farbspritzer oder Mörtelspritzer, Aerosole von Meersalz, atmosphärische Partikel wie Ruß oder Staub, Rückstände von Konservierungsstoffen wie Zellulosekompressen, Strahlgut etc."@de ;
            rdfs:label "Ablagerung"@de ,
                       "Deposit"@en .


###  https://w3id.org/nsd#Detachment
nsd:Detachment rdf:type owl:Class ;
               rdfs:subClassOf nsd:StoneDamage ;
               rdfs:comment "Abtragung von Material des geschädigten Steins."@de ,
                            "Removal of material from the damaged stone."@en ;
               rdfs:label "Ablösung"@de ,
                          "Detachment"@en .


###  https://w3id.org/nsd#DifferentialErosion
nsd:DifferentialErosion rdf:type owl:Class ;
                        rdfs:subClassOf nsd:Erosion ;
                        rdfs:comment "Man spricht von „Differenzieller Erosion“, wenn die Erosion in einem Bereich der Oberfläche nicht mit der gleichen Geschwindigkeit voranschreitet wie in einem anderen. Dadurch verwittert das Gestein ungleichmäßig. Diese Erscheinung wird bei heterogen zusammen gesetzten Gesteinen mit härteren und/oder poröseren Bereichen gefunden. Differenzielle Erosion kann auch als Folge des Angriffs von Flechten auf calcitischen Gesteinen auftreten. Sie wird auf sedimentären wie auf vulkanischen Gesteinen angetroffen. Differenzielle Erosion ist synonym mit „Relief“, d. h. mit der Bildung von unregelmäßigen Formen an der Oberfläche. „Differenzielle Erosion“ kann den „Verlust von Komponenten“ oder den „Verlust von Matrix“ des Gesteins zur Folge haben."@de ,
                                     """occurs when erosion does not proceed at the same rate from one area of the stone to the other. As a result, the stone deteriorates irregularly. This feature is found on heterogeneous stones containing harder and/or less porous zones. It
may also occur as a result of selective lichen attack on calcitic stones. Differential erosion is generally found on sedimentary and volcanic stones. Differential erosion is synonymous with relief formation, i.e. the formation of irregularities on the stone surface. Differential erosion may result in loss of components or loss of matrix of the stone"""@en ;
                        rdfs:label "Differential erosion"@en ,
                                   "Differenzielle Erosion"@de .


###  https://w3id.org/nsd#DiscolorationDeposit
nsd:DiscolorationDeposit rdf:type owl:Class ;
