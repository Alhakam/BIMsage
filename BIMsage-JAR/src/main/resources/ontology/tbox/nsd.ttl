<https://w3id.org/nsd#> rdf:type owl:Ontology ;
                         owl:versionIRI <https://w3id.org/nsd#/0.8.0> ;
                         dcterms:creator [ rdf:type foaf:Person ;
                                           foaf:name "Al-Hakam Hamdan"
                                         ] ,
                                         [ rdf:type foaf:Person ;
                                           foaf:name "Peter Katranuschkov"
                                         ] ,
                                         [ foaf:name "Mushtak Seeaed" ;
                                           rdf:type foaf:Person
                                         ] ;
                         dcterms:description "Ontology for natural stone damages. Extension of the Damage Topology Ontology (DOT)"@en ;
